# Application Configuration
VITE_APP_NAME=Classified Operations Center
VITE_APP_VERSION=1.0.0

# API Configuration (if connecting to backend)
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=10000

# Authentication (for production)
VITE_JWT_SECRET=your-super-secret-jwt-key-here
VITE_SESSION_TIMEOUT=3600000

# External Services (optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://your-sentry-dsn.com

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_MOCK_DATA=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# Database (if using backend)
DATABASE_URL=postgresql://user:password@localhost:5432/operations_db
REDIS_URL=redis://localhost:6379

# Security
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000
